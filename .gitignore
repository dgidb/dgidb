# Ignore bundler config
/.bundle

# Ignore python notebooks
*.ipynb

# Ignore generated data directory
/data
/lib/genome/updaters/data

# Ignore the default SQLite database.
/db/*.sqlite3

# Ignore Ruby version management
.rvmrc
.ruby-version

# Ignore the data dump
/db/data.sql

# Ignore all logfiles and tempfiles.
/log/*.log
/tmp
*.log

# Ignore IDE project directories
.idea
.generators
.agignore
.rakeTasks

# Ignore ds_store and schema
/db/migrate/.DS_Store
/db/schema.rb

/coverage

/scratch

.DS_Store

/public/assets
